{
	"Ionic card": {
		"prefix": "ion-card",
		"body": [
			"<ion-card>",
				"\t<ion-card-header>",
					"\t\t<ion-card-subtitle>Subtitle</ion-card-subtitle>",
					"\t\t<ion-card-title>Title</ion-card-title>",
				"\t</ion-card-header>",
				"\t<ion-card-content>",
				"\t</ion-card-content>",
			"</ion-card>"
		]
	},
	"Ionic list": {
		"prefix": "ion-list",
		"body": [
			"<ion-list>",
				"\t<ion-item lines=\"none\" *ngFor=\"let i of $1\">",
				"\t</ion-item>",
			"</ion-list>"
		]
	},
	"Ionic Refresher": {
		"prefix": "ion-refresher",
		"body": [
			"<ion-refresher slot=\"fixed\" (ionRefresh)=\"refreshData($event)\">",
				"\t<ion-refresher-content></ion-refresher-content>",
			"</ion-refresher-content>"
		]
	},
	"Ionic Buttons": {
		"prefix": "ion-buttons",
		"body": [
			"<ion-buttons slot=\"$1\">",
				"\t<ion-button (click)=\"action()\">",
				"\t\t<ion-icon slot=\"icon-only\" name=\"$2\"></ion-icon>",
				"\t</ion-button>",
			"</ion-buttons>"
		]
	},
	"Ionic Fab Buttons": {
		"prefix": "ion-fab",
		"body": [
			"<ion-fab vertical=\"bottom\" horizontal=\"end\" slot=\"fixed\">",
				"\t<ion-fab-button (click)=\"action()\">",
				"\t\t<ion-icon name=\"$2\"></ion-icon>",
				"\t</ion-fab-button>",
			"</ion-fab>"
		]
	},
	"Ionic Back button": {
		"prefix": "ion-back-buttons",
		"body": [
			"<ion-buttons slot=\"start\">",
			"\t<ion-back-button defaultHref=\"/\"></ion-back-button>",
			"</ion-buttons>"
		]
	},
	"Ionic standard button": {
		"prefix": "ion-button",
		"body": [
			"<ion-button (click)=\"$1\"></ion-button>"
		]
	}
}
