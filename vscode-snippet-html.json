{
	"Ionic card": {
		"prefix": "ion-card",
		"body": [
			"<ion-card>",
				"\t<ion-card-header>",
					"\t\t<ion-card-subtitle>Subtitle</ion-card-subtitle>",
					"\t\t<ion-card-title>Title</ion-card-title>",
				"\t</ion-card-header>",
				"\t<ion-card-content>",
				"\t</ion-card-content>",
			"</ion-card>"
		]
	},
	"Ionic list": {
		"prefix": "ion-list",
		"body": [
			"<ion-list>",
				"\t<ion-item lines=\"none\" *ngFor=\"let i of $1\">",
				"\t</ion-item>",
			"</ion-list>"
		]
	}
}
